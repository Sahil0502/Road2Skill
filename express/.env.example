# Local Development Environment Variables
# Try connection without authentication first
MONGODB_URI=mongodb://localhost:27017/roadmapapp

# If your local MongoDB requires authentication, uncomment and modify this line:
# MONGODB_URI=mongodb://admin:password@localhost:27017/roadmapapp?authSource=admin

# API Keys - REPLACE WITH YOUR ACTUAL KEYS!
GEMINI_API_KEY=your_gemini_api_key_here
YOUTUBE_API_KEY=your_youtube_api_key_here

# Session configuration
SESSION_SECRET=your-local-session-secret-key

# Set NODE_ENV for local development
NODE_ENV=development

# Skip database seeding if needed for local development
SKIP_SEEDING=false
